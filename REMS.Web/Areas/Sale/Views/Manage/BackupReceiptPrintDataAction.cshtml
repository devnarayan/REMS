@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>BackupReceiptPrintDataAction</title>
    @*<script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/moment-with-locales.min.js"></script>*@
    <script src="~/Scripts/angular.js"></script>
    @*<script src="~/Scripts/ngController/ManagePayjs.js"></script>*@
    <script src="~/AgController/ManagePayJs.js"></script>
    <link rel="stylesheet" type="text/css" media="screen" href="~/Content/css/style.css">
    <style>
        table {
            border: 2px solid gray;
            width: 100%;
        }

            table thead {
                border: 1px solid gray;
                background-color: rgb(59, 57, 61);
                padding: 0;
                margin: 0;
                color: rgb(247, 13, 13);
            }

            table tbody tr {
                border-bottom: 1px solid;
                border-color: gray;
            }

        td {
            border-bottom: 1px solid gray;
            border-left: 1px solid gray;
        }

        tr {
            border-bottom: 1px solid red;
        }
    </style>

</head>
<body data-ng-app="ManageApp">
    <div id="ReceiptPritable" data-ng-controller="ManageController">
        <div data-ng-init="GetBackupReceiptPrintDataInit()">
            <div id="loading">
                <img id="loading-image" src="~/images/loading.gif" alt="Loading..." />
            </div>
            <input type="hidden" id="hidID" name="hidID" value="@ViewBag.ID" />
            <table>
                <thead>
                    <tr>
                        <td>PayDate</td>
                        <td>Amount</td>
                        <td>Owner</td>
                        <td>PropertyName</td>
                        <td>ReceiptNo</td>
                        <td>PayMode</td>
                        <td>ChequeNo</td>
                        <td>BankName</td>
                        <td>ChequeDate</td>
                        <td>Status</td>
                    </tr>
                </thead>
                <tbody>
                    <tr data-ng-repeat="sr in SearchList">
                        <td>{{sr.PaymentDate}}</td>
                        <td>{{sr.Amount}}</td>
                        <td>{{sr.CustomerName}}</td>
                        <td>{{sr.FlatName}}</td>
                        <td>{{sr.PaymentNo}}</td>
                        <td>{{sr.PaymentMode}}</td>
                        <td>{{sr.CheckNo}}</td>
                        <td>{{sr.BankName}}</td>
                        <td>{{sr.ChequeDate}}</td>
                        <td>{{sr.PaymentStatus}}</td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr><td></td><td><b>Total</b></td><td><b>{{TotalSearchAmount}}</b></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr>
                </tfoot>
            </table>
        </div>
    </div>
    <script src="~/Scripts/jquery-2.1.3.min.js"></script>
    @*<script src="~/Content/js/jquery-1.7.2.min.js"></script>
    <script src="~/Content/js/jquery-ui-1.8.19.custom.min.js"></script>
    <link href="~/Content/css/jquery-ui-1.8.19.custom.css" rel="stylesheet" />*@
</body>
</html>